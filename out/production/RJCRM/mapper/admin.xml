<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="server.AdminServer">
    <select id="findByNameAndPass"  resultType="admin">
        select id,admin_name from tb_admin where admin_name=#{arg0} and admin_password=#{arg1}
    </select>
</mapper>


